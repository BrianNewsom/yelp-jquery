div.row
    div.five.columns
        strong= "Name"
    div.two.columns
        strong= "City"
    div.one.columns
        strong= "State"
    div.one.columns
        strong= "Stars"
    div.three.columns
        strong= "Appointment Required?"

each item in items 
    div.row
        div.five.columns
            a(href="#", onClick='viewDoctor("' + item.id + '")')
                = item.name
        div.two.columns
            = item.location.city
        div.one.columns
            = item.location.state
        div.one.columns
            img(src=item.rating_img_url, border='0')
        div.three.columns
            if item.attributes
                if (item.attributes["By Appointment Only"])
                    = "Yes"
                else
                    = "No"
