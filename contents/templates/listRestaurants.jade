div.row
    div.four.columns
        strong= "Name"
    div.two.columns
        strong= "City"
    div.two.columns
        strong= "State"
    div.two.columns
        strong= "Price Range"
    div.two.columns
        strong= "Stars"
each item in items  
    div.row
        div.four.columns
            a(href="#", onClick='viewRestaurant("' + item.business_id + '")')
                = item.name
        div.two.columns
            = item.city
        div.two.columns
            = item.state
        div.two.columns
            ="No Data"
        div.two.columns
            = item.stars
            |  stars
